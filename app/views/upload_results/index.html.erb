<h1>Listing upload_results</h1>

<table>
  <tr>
    <th>Kind</th>
    <th>Error report path</th>
    <th>Body file name</th>
    <th>Body content type</th>
    <th>Body file size</th>
    <th>Total size</th>
    <th>Error size</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @upload_results.each do |upload_result| %>
  <tr>
    <td><%= upload_result.kind %></td>
    <td><%= upload_result.error_report_path %></td>
    <td><%= upload_result.body_file_name %></td>
    <td><%= upload_result.body_content_type %></td>
    <td><%= upload_result.body_file_size %></td>
    <td><%= upload_result.total_size %></td>
    <td><%= upload_result.error_size %></td>
    <td><%= link_to 'Show', upload_result %></td>
    <td><%= link_to 'Edit', edit_upload_result_path(upload_result) %></td>
    <td><%= link_to 'Destroy', upload_result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Upload result', new_upload_result_path %>
